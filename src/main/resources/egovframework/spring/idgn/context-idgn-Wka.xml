<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <bean name="egovWkaIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="dataSource" />
        <property name="strategy"   ref="wkaStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="TN_FEIS_COPSEQ"/>
        <property name="tableName"  value="WKA_ID"/>
    </bean>
    <bean name="wkaStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="WKA_" />
        <property name="cipers"   value="10" />
        <property name="fillChar" value="0" />
    </bean>

</beans>