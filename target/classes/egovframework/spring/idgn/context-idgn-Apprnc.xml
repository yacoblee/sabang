<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">

    <!-- Web Log... START-->            
    <bean name="egovApprncIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="dataSource" />
        <property name="strategy"   ref="apprncChckStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="TN_FEIS_COPSEQ"/>
        <property name="tableName"  value="APPRNC_ID"/>
    </bean>
    <bean name="apprncChckStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="APPRNC_" />
        <property name="cipers"   value="13" />
        <property name="fillChar" value="0" />
    </bean>     
    <!-- Web Log... END-->  

</beans>